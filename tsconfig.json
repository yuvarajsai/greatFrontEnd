{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["DOM", "ESNext"],
		"jsx": "react-jsx",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"outDir": "dist"
	},
	"include": ["react"]
}
